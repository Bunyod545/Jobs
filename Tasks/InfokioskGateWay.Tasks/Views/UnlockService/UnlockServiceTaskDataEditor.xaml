<dataEditor:BaseTaskDataEditor x:Class="InfokioskGateWay.Tasks.Views.UnlockService.UnlockServiceTaskDataEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:dataEditor="clr-namespace:Jobs.Tasks.Common.Logics.DataEditor;assembly=Jobs.Tasks.Common"
        mc:Ignorable="d"
        Title="TestTaskDataEditor" Height="104.771" Width="593.979" Loaded="TaskDataEditor_OnLoaded" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Url" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="53,13,0,0" TextWrapping="Wrap" Text="{Binding UnlockServiceUrl}" VerticalAlignment="Top" Width="523"/>

        <Button Content="Save" HorizontalAlignment="Left" Margin="421,41,0,0" VerticalAlignment="Top" Width="75" Click="SaveButton_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="501,41,0,0" VerticalAlignment="Top" Width="75" Click="CancelButton_Click"/>
    </Grid>
</dataEditor:BaseTaskDataEditor>
